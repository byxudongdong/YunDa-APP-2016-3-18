<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@drawable/bg1"
    >
    <LinearLayout 
        android:id="@+id/LinearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        >
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:layout_weight="1"
              android:textSize="20dp"
              android:layout_marginTop="5dp"
            android:text="重量"/>
        <CheckBox 
            android:id="@+id/loadcheckbox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
              android:layout_weight="1"
            />
        <TextView 
            android:id="@+id/loadTextView1"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:background="#E0E0E0"
               android:singleLine="true"
            android:text="                     "
             android:layout_weight="1"
            />
    </LinearLayout>
	 <RelativeLayout 
	     android:id="@+id/RelativeLayout1"
	     android:layout_width="match_parent"
	     android:layout_below="@id/LinearLayout1"
	     android:layout_height="40dp"
	     >

	     <TextView
	         android:id="@+id/textView1"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_alignParentLeft="true"
	         android:layout_alignTop="@+id/editText1"
	         android:layout_marginTop="5dp"
	          android:textSize="20dp"
	         android:text="物品类别" />
	     
	     <Spinner
	         android:id="@+id/loadspinner1"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_alignTop="@+id/textView1"
	         android:layout_marginLeft="5dp"
	         android:layout_toRightOf="@+id/textView1" 
	         android:paddingTop="1dp"
	         android:entries="@array/languages"
	         />
	     
	     <TextView
	         android:id="@+id/textView2"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_alignBottom="@+id/loadeditText1"
	         android:layout_alignLeft="@+id/loadspinner1"
	         android:layout_marginLeft="90dp"
	         android:layout_marginTop="5dp"
	          android:textSize="20dp"
	         android:text="装车位" />
	      <EditText
	         android:id="@+id/loadeditText1"
	         android:layout_width="wrap_content"
	         android:layout_height="wrap_content"
	         android:layout_alignParentRight="true"
	         android:layout_centerVertical="true"
	         android:layout_toRightOf="@+id/textView2"
	         android:background="@drawable/txtborder"
	         android:ems="10" >
	         <requestFocus />
	     </EditText>

	     
	 </RelativeLayout>
  <LinearLayout 
        android:id="@+id/LinearLayout3"
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_below="@id/RelativeLayout1"
        >
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:textSize="20dp"
            android:text="发车凭证"/>
      <EditText 
          android:id="@+id/loadEdit"
          android:layout_width="200dp"
          android:layout_weight="1"
         android:layout_marginLeft="10dp"
          android:background="@drawable/txtborder"
          android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout 
        android:id="@+id/LinearLayout4"
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_below="@id/LinearLayout3"
        >
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:layout_marginTop="5dp"
              android:textSize="20dp"
            android:text="下一站   "/>
        <EditText 
            android:id="@+id/loadEditText "
          android:layout_width="100dp"
         android:layout_marginLeft="10dp"
          android:background="@drawable/txtborder"
          android:layout_marginTop="5dp"
          android:layout_weight="1"
          android:layout_height="wrap_content"/>
      <TextView 
          android:id="@+id/loadTextView2" 
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:background="#E0E0E0"
            android:singleLine="true"
            android:layout_marginLeft="1dp"
            android:text="                     "
             android:layout_weight="1"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayout5"
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_below="@id/LinearLayout4"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:paddingTop="5dp"
              android:textSize="20dp"
            android:text="运单号码" />

        <EditText
            android:id="@+id/loadaddbutton"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
             android:paddingTop="5dp"
             android:layout_weight="1"
            android:background="@drawable/txtborder" />

        <Button
            android:id="@+id/loadbutton1"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/login_button_selector"
            android:paddingBottom="5dp"
            android:textSize="15dp"
            android:text="添加" />
    </LinearLayout>
<LinearLayout
        android:id="@+id/LinearLayout6"
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_below="@id/LinearLayout5"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:paddingTop="5dp"
              android:textSize="20dp"
            android:text="已扫描数" />

        <EditText
            android:id="@+id/loadEditText4"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
             android:paddingTop="5dp"
              android:layout_weight="1"
            android:background="@drawable/txtborder" />

        <Button
            android:id="@+id/loaddeletebutton"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/login_button_selector"
            android:paddingBottom="5dp"
            android:textSize="15dp"
            android:text="删除" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/LinearLayout7"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@id/LinearLayout6"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >
	<ListView 
	    android:id="@+id/loadlistview"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    >
	    
	</ListView>

        
    </LinearLayout>
    <LinearLayout
        android:id="@+id/LinearLayout8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
         android:layout_alignParentBottom="true"
         android:layout_centerInParent="true"
        android:orientation="horizontal" >
        <Button
            android:id="@+id/loadfinish"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:background="@drawable/login_button_selector"
            android:paddingBottom="5dp"
            android:textSize="15dp"
            android:text="返回" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
         android:textSize="50dp"
        android:text="0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_toRightOf="@+id/textView3"
         android:textSize="50dp"
        android:text="/0" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/textView4"
        />

</RelativeLayout>
